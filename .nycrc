{
  "require": [
    "babel-register"
  ],
  "temp-directory": "coverage/.nyc_output",
  "report-dir": "coverage",
  "reporter": [
    "lcov",
    "text-summary",
    "json"
  ],
  "check-coverage": true,
  "per-file": false,
  "lines": 90,
  "statements": 90,
  "functions": 90,
  "branches": 90,
  "cache": false,
  "include": [
    "**/src/**/*.js"
  ],
  "exclude": [
    "**/lib/**/*.js",
    "**/test/**/*.js"
  ]
}
